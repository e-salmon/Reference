{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "dark": "#262b44",
    "path": "#feae34",
    "git": "#fee761",
    "git-changes": "#f77622",
    "git-ab": "#e43b44",
    "git-ahead": "#2ce8f5",
    "git-behind": "#f77622",
    "status-ok": "#0095e9",
    "status-bad": "#ff0044",
    "time": "#0095e9",
    "white": "#FFFFFF",
    "tan": "#CC3802",
    "teal": "#047E84",
    "plum": "#9A348E",
    "blush": "#DA627D",
    "salmon": "#FCA17D",
    "sky": "#86BBD8",
    "teal_blue": "#33658A"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:plum",
          "foreground": "p:white",
          "properties": {
            "time_format": "15:04"
          },
          "style": "powerline",
          "template": "{{ .CurrentDate | date .Format }}",
          "powerline_symbol": "\ue0b0",
          "type": "time"
        },
        {
          "background": "p:path",
          "foreground": "p:dark",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "p:git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changes{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-ab{{ end }}",
            "{{ if gt .Ahead 0 }}p:ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:behind{{ end }}"
          ],
          "foreground": "#262b44",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:status-ok",
          "background_templates": [
            "{{ if gt .Code 0 }}p:status-bad{{ end }}"
          ],
          "foreground": "p:white",
          "properties": {
            "always_enabled": true
          },
          "template": "Â ",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}